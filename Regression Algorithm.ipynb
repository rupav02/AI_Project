    "def linear_regression(X_test, y_test):\n",
    "    w = 0\n",
    "    b = 0\n",
    "    alpha = 10 ** -20\n",
    "    iterations = 10000\n",
    "\n",
    "    for i in range(iterations):\n",
    "        dJ_dw = (1/len(X_test))*sum((w*X_test + b - y_test)*X_test)\n",
    "        dJ_db = (1/len(X_test))*sum(w*X_test + b - y_test)\n",
    "        w = w - alpha*dJ_dw\n",
    "        b = b - alpha*dJ_db\n",
    "    \n",
    "    parameters = (w, b)\n",
    "    return parameters"
   ]
